<!DOCTYPE html>
<html>
  <head>
    <title>Pi Prover</title>
    <!--Angular scripts-->
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
    <script src="/javascripts/angularApp.js"></script>
    <link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
    <script src="/stylesheets/style.css" rel="stylesheet"></script>

    <!--Graph scripts-->
    <meta charset="utf-8">
    <script src="bower_components/d3/d3.js"></script>
    <script src="bower_components/nvd3/build/nv.d3.js"></script>
    <script src="bower_components/angular-nvd3/dist/angular-nvd3.js"></script>
    <link rel="stylesheet" href="bower_components/nvd3/build/nv.d3.css">

    <!--Slider scripts-->
    <script src="bower_components/angularjs-slider/dist/rzslider.js"></script>
    <link rel="stylesheet" href="bower_components/angularjs-slider/dist/rzslider.css"/>


  </head>
  <body ng-app="piProver">
    <div ng-controller="MainCtrl" class="big-container">
      <h1>Pi Prover</h1>
      <nvd3 options="options" data="data"></nvd3>
      <rzslider rz-slider-model="slider.value" rz-slider-options="slider.options"></rzslider>
       x = {{x}}
    </div>
  </body>
</html>
